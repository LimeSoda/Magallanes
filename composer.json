      "name": "Georg Großberger",
      "email": "georg.grossberger@cyberhouse.at",
      "homepage": "https://www.cyberhouse.at",
      "role": "Developer"
    }
  ],
  "keywords": [
    "deployment",
    "PHP"
  ],
  "support": {
    "email": "georg.grossberger@cyberhouse.at",
    "issues": "https://github.com/cyberhouse/Magallanes/issues",
    "source": "https://github.com/cyberhouse/Magallanes",
    "docs": "https://github.com/cyberhouse/Magallanes/wiki"
  },
  "require": {
    "php": ">=5.5",
    "symfony/console": "^2.8.3",
    "symfony/yaml": "^2.6.1",
    "pimple/pimple": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "satooshi/php-coveralls": "~0.6",
    "php-mock/php-mock": "~1.0",
    "cyberhouse/phpstyle": "^0.1.1"
  },
  "suggest": {
    "ext-yaml": "*"
  },
  "autoload": {
    "psr-4": {
      "Mage\\": "./src/Mage",
      "Task\\": [
        ".mage/tasks",
        "../../../.mage/tasks"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MageTest\\": "./tests"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "bin": [
    "bin/mage"
  ]
}
